{
  "name": "@urql/storybook-addon",
  "version": "1.0.3",
  "description": "Make stories in Storybook with urql",
  "sideEffects": false,
  "homepage": "https://github.com/FormidableLabs/urql/tree/main/packages/storybook-addon",
  "bugs": "https://github.com/FormidableLabs/urql/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/urql.git",
    "directory": "packages/storybook-addon"
  },
  "keywords": [
    "addon",
    "storybook",
    "urql",
    "formidablelabs",
    "preact",
    "react"
  ],
  "main": "dist/urql-storybook-addon.js",
  "module": "dist/urql-storybook-addon.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "dist/",
    "preact/"
  ],
  "exports": {
    ".": {
      "import": "./dist/urql-storybook-addon.js",
      "require": "./dist/urql-storybook-addon.mjs",
      "types": "./dist/types/decorator/react.d.ts",
      "source": "./src/decorator/react.ts"
    },
    "./preact": {
      "import": "./dist/urql-storybook-addon-preact.js",
      "require": "./dist/urql-storybook-addon-preact.js",
      "types": "./dist/types/decorator/preact.d.ts",
      "source": "./src/decorator/preact.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "check": "tsc --noEmit",
    "lint": "eslint --ext=js,jsx,ts,tsx .",
    "build": "rollup -c ../../scripts/rollup/config.js",
    "prepare": "node ../../scripts/prepare/index.js",
    "postbuild": "cp src/index.d.ts dist/index.d.ts",
    "prepublishOnly": "run-s clean build"
  },
  "devDependencies": {
    "@urql/preact": "latest",
    "@storybook/preact": ">= 6.0.0",
    "@storybook/react": ">= 6.0.0",
    "@types/webpack-env": "~1.15.3",
    "preact": "^10.4.8",
    "react": "^16.13.1",
    "urql": "latest",
    "graphql": "latest",
    "wonka": "latest"
  },
  "optionalDependencies": {
    "@storybook/addons": ">= 6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
