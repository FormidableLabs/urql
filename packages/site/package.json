{
  "name": "urql-docs",
  "version": "6.4.1",
  "description": "Documentation site for urql",
  "main": "static.config.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/urql-docs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/urql-docs.git"
  },
  "homepage": "https://github.com/FormidableLabs/urql-docs#readme",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "stage-debug": "react-static build --staging --debug",
    "serve": "serve dist -l 3000",
    "prettier-all": "npx prettier --write '{src,static-config-helpers,static-config-parts,static.config,scripts,lib}/**/*.{js,jsx}'",
    "preversion": "yarn run lint && yarn run build",
    "format": "yarn run lint:fix",
    "stage-and-serve": "run-s stage serve"
  },
  "babel": {
    "extends": "react-static/.babelrc",
    "plugins": [
      "babel-plugin-styled-components"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "../../scripts/eslint/js-preset.js"
    ]
  },
  "dependencies": {
    "chokidar": "2.0.3",
    "formidable-oss-badges": "0.2.1",
    "history": "^4.7.2",
    "klaw": "^2.1.1",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-document-meta": "^2.0.3",
    "react-dom": "^16.0.0",
    "react-ga": "^2.4.1",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-router": "^4.2.0",
    "react-router-ga": "^1.0.0",
    "react-static": "^5.9.12",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "config": "^3.0.0",
    "gatsby-remark-prismjs": "^3.2.2",
    "github-slugger": "^1.2.1",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "raw-loader": "^1.0.0",
    "react-static-plugin-styled-components": "^6.0.0-beta.28",
    "remark": "^10.0.1",
    "remark-frontmatter": "^1.3.0",
    "remark-html": "^8.0.0",
    "remark-kbd": "^1.0.16",
    "remark-slug": "^5.1.1",
    "remark-toc": "^5.1.0",
    "unist-util-select": "^1.5.0",
    "unist-util-visit": "^1.4.0",
    "url-loader": "^1.1.2"
  }
}
